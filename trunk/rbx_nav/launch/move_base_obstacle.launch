<launch>

  <include file="$(find rbx_nav)/launch/move_base.launch" />
  
  <!-- Run the map server with a blank map -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find rbx_nav)/maps/blank_map_with_obstacle.yaml"/>
    
  <!-- Run a static transform between /odom and /map and /map and /world -->
  <node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster" args="0 0 0 0 0 0 /odom /map 100" />
    
</launch>
